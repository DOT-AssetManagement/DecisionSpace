@page
@model DSS.Web.Pages.SetUp.OtherParameter.EditcshtmlModel

@{
    Layout = "Shared/AdminLayout";
    ViewData["Menu"] = "DSSAdminMenus.Setup";
    ViewData["SubMenu"] = "OtherParameter";
}
<head>
    <title>Edit Other Parameter</title>
</head>
<style>
    .card {
        margin-top: 50px;
        height: 400px; 
</style>

<div class="container">
    <div class="row justify-content-center">
        <div class="col-md-12">
            <div class="card">
                <div class="card-header">
                    <h3 class="card-title">Edit Other Parameter</h3>
                </div>
                <div class="card-body">
                    <form method="post">

                        <div class="form-group row">
                            <label for="Code" class="col-sm-3 col-form-label">Code:</label>
                            <div class="col-sm-9">
                                <input type="text" class="form-control" required="" placeholder="Code" name="Code" value="@Model.otherParameterDto.Code" readonly>
                            </div>
                        </div>
                        <div class="form-group row">
                            <label for="Description" class="col-sm-3 col-form-label">Description:</label>
                            <div class="col-sm-9">
                                <input type="text" class="form-control" required="" placeholder="Description" name="Description" value="@Model.otherParameterDto.Description">
                            </div>
                        </div>
                        <div class="form-group row">
                            <label for="Value" class="col-sm-3 col-form-label">Value:</label>
                            <div class="col-sm-9">
                                <input type="number" class="form-control" required="" placeholder="Value" name="Val" value="@Model.otherParameterDto.Val">
                            </div>
                            <input type="datetime" hidden class="form-control" name="CreatedBy" value="@Model.otherParameterDto.CreatedBy">
                        </div><br />
                        <div class="form-group row mt-3">
                            <div class="col-md-9 offset-md-3">
                                <a href="/SetUp/OtherParameter/List" class="btn btn-secondary">Cancel</a>
                                <button type="submit" class="btn btn-info">Submit</button>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>




