<script>
    function validateWholeNumberInput(input) {
        input.value = input.value.replace(/\D/g, '');
    }

    function validateDecimalInput(input) {
        input.value = input.value.replace(/[^0-9.]/g, '');
        input.value = input.value.replace(/(\..*)\./g, '$1');
        input.value = input.value.replace(/^(\d+)\.(\d{2}).*$/, '$1.$2');
    }


    $(document).on("change", "#AssetType", function () {
        if($(this).val() == "B"){
            $(".BRIDGE_ID").show();
            $(".BRKEY").show();
        }else if($(this).val() == ""){
            $(".BRIDGE_ID").hide();
            $(".BRKEY").hide();
        }else{
            $(".BRIDGE_ID").hide();
            $(".BRKEY").hide();
        }
    });
    $("#AssetType").trigger("change");


    $(document).on("change", "#District", function () {

        $("#Cnty").html("<option value=''>Select</option>");
        $("#Route").html("<option value=''>Select</option>");

        $.ajax({
            url: '/WorkCandidates/Create?handler=GetCounties&id=' + $(this).val(),
            type: 'GET',
            dataType: 'json',
            success: function (data) {
                console.log('Data:', data);
                $.each(data, function (key, value) {

                    if ('@Model.WorkCandidates.Cnty' == value.cnty) {
                        $("#Cnty").append("<option value='" + value.cnty + "' selected>" + value.countyName + "</option>");
                        $("#Cnty").trigger("change");
                    }
                    else
                        $("#Cnty").append("<option value='" + value.cnty + "'>" + value.countyName + "</option>");
                });
            },
            error: function (xhr, status, error) {
                console.error('Error :', error);
            }
        });
    });
    $("#District").trigger("change");


    $(document).on("change", "#Cnty", function () {
        $("#Route").html("<option value=''>Select</option>");

        $.ajax({
            url: '/WorkCandidates/Create?handler=GetRoutes&Countyid=' + $(this).val() + "&Districtid=" + $("#District").val(),
            type: 'GET',
            dataType: 'json',
            success: function (data) {
                console.log('Data:', data);
                $.each(data, function (key, value) {    
                    if ('@Model.WorkCandidates.Route' == value.rte) {
                        $("#Route").append("<option value='" + value.rte + "' selected>" + value.rte + "</option>");
                        $("#Route").trigger("change");
                    }else{
                        $("#Route").append("<option value='" + value.rte + "'>" + value.rte + "</option>");
                    }
                    
                });
            },
            error: function (xhr, status, error) {
                console.error('Error :', error);
            }
        });
    });
    
    
    $.ajax({
        url: '/WorkCandidates/Create?handler=GetBPN',
        type: 'GET',
        dataType: 'json',
        success: function (data) {
            console.log('Data:', data);

            $("#BPN").html("<option value=''>Select</option>");
            $.each(data, function (key, value) {
                if ('@Model.WorkCandidates.BPN' == value.code) {
                    $("#BPN").append("<option value='" + value.code + "' selected>" + value.longDescription + "</option>");
                }else{
                    $("#BPN").append("<option value='" + value.code + "'>" + value.longDescription + "</option>");
                }
            });
        },
        error: function (xhr, status, error) {
            console.error('Error :', error);
        }
    });


    $(document).on("change", "#Route", function () {
        $("#Section").html("<option value=''>Select</option>");

        $.ajax({
            url: '/WorkCandidates/Create?handler=GetSectionPairs&Countyid=' + $("#Cnty").val() + "&Districtid=" + $("#District").val() + "&Route=" + $(this).val(),
            type: 'GET',
            dataType: 'json',
            success: function (data) {
                console.log('Data:', data);
                $.each(data, function (key, value) {
                    if ('@Model.WorkCandidates.FromSection' == value.fromSection && '@Model.WorkCandidates.ToSection' == value.toSection) {
                        $("#Section").append("<option value='" + value.fromTo + "' selected>" + value.fromTo + "</option>");
                    }else{
                        $("#Section").append("<option value='" + value.fromTo + "'>" + value.fromTo + "</option>");
                    }
                });
            },
            error: function (xhr, status, error) {
                console.error('Error :', error);
            }
        });

        $.ajax({
            url: '/WorkCandidates/Create?handler=GetBrkeyAndBMSID&Countyid=' + $("#Cnty").val() + "&Districtid=" + $("#District").val() + "&Route=" + $(this).val(),
            type: 'GET',
            dataType: 'json',
            success: function (data) {
                console.log('Data:', data);
                $("#BRKEY").html("<option value=''>Select</option>");
                $.each(data, function (key, value) {
                    $("#BRKEY").append("<option bmsid='" + value.bmsid + "' value='" + value.brkey + "'>" + value.brkey + "</option>");
                });
            },
            error: function (xhr, status, error) {
                console.error('Error :', error);
            }
        });
    });

    $(document).on("change", "#BRKEY", function () {
        $("#BRIDGE_ID").val($(this).find('option:selected').attr('bmsid'));
    });


    $("#Degree_of_Safety_Improvement").val(@Model.WorkCandidates.Degree_of_Safety_Improvement);
    $("#Degree_of_Mobility_Improvement").val(@Model.WorkCandidates.Degree_of_Mobility_Improvement);
    $("#Degree_of_Environmental_Improvement").val(@Model.WorkCandidates.Degree_of_Environmental_Improvement);
    $("#Year").val(@Model.WorkCandidates.Year);
    $("#MinYear").val(@Model.WorkCandidates.MinYear);
    $("#MaxYear").val(@Model.WorkCandidates.MaxYear);

</script>