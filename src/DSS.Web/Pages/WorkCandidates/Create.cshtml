                                <label>
                                    <input type="radio" class="" name="input.HSIP_Eligible" id="HSIP_Eligible" value="0" @(Model.WorkCandidates.HSIP_Eligible == 0 ? "checked" : "") @(Model.WorkCandidates.HSIP_Eligible == null ? "checked" : "")> No
                                </label>
                    </div>
                    <div class="col-lg-2 col-md-3 mb-5">
                        <label for="Supports_Hazard_Mitigation" class="form-label">Supports Hazard Mitigation</label>
                                <br>
                                <label>
                                    <input type="radio" class="" name="input.Supports_Hazard_Mitigation" id="Supports_Hazard_Mitigation" value="1" @(Model.WorkCandidates.Supports_Hazard_Mitigation == 1 ? "checked" : "")> Yes
                                </label>
                                <label>
                                    <input type="radio" class="" name="input.Supports_Hazard_Mitigation" id="Supports_Hazard_Mitigation" value="0" @(Model.WorkCandidates.Supports_Hazard_Mitigation == 0 ? "checked" : "") @(Model.WorkCandidates.Supports_Hazard_Mitigation == null ? "checked" : "")> No
                                </label>
                    </div>
                    <div class="col-lg-2 col-md-3 mb-5">
                        <label for="Improves_Access_to_Emergency_Services" class="form-label">Improves Access to Emergency Services</label>
                             
                                <br>
                                <label>
                                    <input type="radio" class="" name="input.Improves_Access_to_Emergency_Services" id="Improves_Access_to_Emergency_Services" value="1" @(Model.WorkCandidates.Improves_Access_to_Emergency_Services == 1 ? "checked" : "")> Yes
                                </label>
                                <label>
                                    <input type="radio" class="" name="input.Improves_Access_to_Emergency_Services" id="Improves_Access_to_Emergency_Services" value="0" @(Model.WorkCandidates.Improves_Access_to_Emergency_Services == 0 ? "checked" : "") @(Model.WorkCandidates.Improves_Access_to_Emergency_Services == null ? "checked" : "")> No
                                </label>
                    </div>
                    <div class="col-lg-2 col-md-3 mb-5">
                         <label for="Supported_by_RSA_Safety_Study" class="form-label">Supported by RSA Safety Study</label>
                              

                                <br>
                                <label>
                                    <input type="radio" class="" name="input.Supported_by_RSA_Safety_Study" id="Supported_by_RSA_Safety_Study" value="1" @(Model.WorkCandidates.Supported_by_RSA_Safety_Study == 1 ? "checked" : "")> Yes
                                </label>
                                <label>
                                    <input type="radio" class="" name="input.Supported_by_RSA_Safety_Study" id="Supported_by_RSA_Safety_Study" value="0" @(Model.WorkCandidates.HSIP_Eligible == 0 ? "checked" : "") @(Model.WorkCandidates.Supported_by_RSA_Safety_Study == null ? "checked" : "")> No
                                </label>
                    </div>
                     @if (@Model.WorkCandidateId != null)
                            {
                                <div class="col-lg-2 col-md-3 mb-5">
                                    <label for="Predicted_Safety_Benefit" class="form-label">Predicted Safety Benefit</label>

                                    <div class="input-group"> 
                                        <span class="input-group-text">$</span>
                                        <input type="text" class="form-control" name="input.Predicted_Safety_Benefit" id="Predicted_Safety_Benefit" value="@Model.WorkCandidates.Predicted_Safety_Benefit" readonly disabled>
                                    </div>

                                </div>
                                <div class="col-lg-2 col-md-3 mb-5">
                                    <label for="Safety_Score" class="form-label">Safety Score</label>
                                    <input type="text" class="form-control" name="input.Safety_Score" id="Safety_Score" value="@(Model.WorkCandidates.Safety_Score?.ToString("0.00") ?? "")" disabled>
                                </div>
                            }
                        </div>
                </div>
                <div id="tabs-mobility">
                        <div class="row mb-3">
                            <div class="col-lg-2 col-md-3 mb-5">
                                <label for="Travel_Time_Savings" class="form-label">
                                    Travel Time Savings(Hours/Year)
                                </label>
                                <input type="text" class="form-control" name="input.Travel_Time_Savings" id="Travel_Time_Savings" value="@Model.WorkCandidates.Travel_Time_Savings" oninput="validateWholeNumberInput(this)">
                            </div>
                            <div class="col-lg-2 col-md-3 mb-5">
                                <label for="Operating_Cost_Savings" class="form-label">
                                    Operating Cost Savings($/Year)
                                </label>
                                
                                <div class="input-group">
                                    <span class="input-group-text">$</span>
                                    <input type="text" class="form-control" name="input.Operating_Cost_Savings" id="Predicted_Operating_Cost_SavingsSafety_Benefit" value="@Model.WorkCandidates.Operating_Cost_Savings" oninput="validateDecimalInput(this)">
                                </div>
                            </div>
                            <div class="col-lg-2 col-md-3 mb-5">
                                <label for="Degree_of_Mobility_Improvement" class="form-label">Degree of Mobility Improvement</label>
                                <select class="form-control" name="input.Degree_of_Mobility_Improvement" id="Degree_of_Mobility_Improvement" value="">
                                    <option value="">Select</option>
                                    <option value="2">Large</option>
                                    <option value="1">Small</option>
                                    <option value="0">None</option>
                                    </select>
                            </div>
                          

                           


                            <div class="col-lg-2 col-md-3 mb-5">
                                <label for="Freight_Improvement" class="form-label">Freight Improvement</label>
                                
                                <br>
                                <label>
                                    <input type="radio" class="" name="input.Freight_Improvement" id="Freight_Improvement" value="1" @(Model.WorkCandidates.Freight_Improvement == 1 ? "checked" : "")> Yes
                                </label>
                                <label>
                                    <input type="radio" class="" name="input.Freight_Improvement" id="Freight_Improvement" value="0" @(Model.WorkCandidates.Freight_Improvement == 0 ? "checked" : "") @(Model.WorkCandidates.Freight_Improvement == null ? "checked" : "")> No
                                </label>
                            </div>
                            <div class="col-lg-2 col-md-3 mb-5">
                                <label for="Facilitates_Transit_Use" class="form-label">Facilitates Transit Use</label>
                               

                                <br>
                                <label>
                                    <input type="radio" class="" name="input.Facilitates_Transit_Use" id="Facilitates_Transit_Use" value="1" @(Model.WorkCandidates.Facilitates_Transit_Use == 1 ? "checked" : "")> Yes
                                </label>
                                <label>
                                    <input type="radio" class="" name="input.Facilitates_Transit_Use" id="Facilitates_Transit_Use" value="0" @(Model.WorkCandidates.Facilitates_Transit_Use == 0 ? "checked" : "") @(Model.WorkCandidates.Facilitates_Transit_Use == null ? "checked" : "")> No
                                </label>
                            </div>
                            <div class="col-lg-2 col-md-3 mb-5">
                                <label for="Facilitates_Active_Transportation" class="form-label">Facilitates Active Transportation</label>
                               

                                <br>
                                <label>
                                    <input type="radio" class="" name="input.Facilitates_Active_Transportation" id="Facilitates_Active_Transportation" value="1" @(Model.WorkCandidates.Facilitates_Active_Transportation == 1 ? "checked" : "")> Yes
                                </label>
                                <label>
                                    <input type="radio" class="" name="input.Facilitates_Active_Transportation" id="Facilitates_Active_Transportation" value="0" @(Model.WorkCandidates.Facilitates_Active_Transportation == 0 ? "checked" : "") @(Model.WorkCandidates.Facilitates_Active_Transportation == null ? "checked" : "")> No
                                </label>
                            </div>
                            <div class="col-lg-2 col-md-3 mb-5">
