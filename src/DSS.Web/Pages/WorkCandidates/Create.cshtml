                                <input type="text" class="form-control" name="input.Bike_Lanes_Constructed" id="Bike_Lanes_Constructed" value="@Model.WorkCandidates.Bike_Lanes_Constructed" oninput="validateWholeNumberInput(this)">
                            </div>
                           
                            <div class="col-lg-2 col-md-3 mb-5">
                                <label for="Support_for_Job_Growth" class="form-label">Support for Job Growth</label>
                                
                                <br>
                                <label>
                                    <input type="radio" class="" name="input.Support_for_Job_Growth" id="Support_for_Job_Growth" value="1" @(Model.WorkCandidates.Support_for_Job_Growth == 1 ? "checked" : "")> Yes
                                </label>
                                <label>
                                    <input type="radio" class="" name="input.Support_for_Job_Growth" id="Support_for_Job_Growth" value="0" @(Model.WorkCandidates.Support_for_Job_Growth == 0 ? "checked" : "") @(Model.WorkCandidates.Support_for_Job_Growth == null ? "checked" : "")> No
                                </label>
                            </div>
                            <div class="col-lg-2 col-md-3 mb-5">
                                <label for="Support_for_Tourism" class="form-label">Support for Tourism</label>
                                
                                <br>
                                <label>
                                    <input type="radio" class="" name="input.Support_for_Tourism" id="Support_for_Tourism" value="1" @(Model.WorkCandidates.Support_for_Tourism == 1 ? "checked" : "")> Yes
                                </label>
                                <label>
                                    <input type="radio" class="" name="input.Support_for_Tourism" id="Support_for_Tourism" value="0" @(Model.WorkCandidates.Support_for_Tourism == 0 ? "checked" : "") @(Model.WorkCandidates.Support_for_Tourism == null ? "checked" : "")> No
                                </label>
                            </div>
                            <div class="col-lg-2 col-md-3 mb-5">
                                <label for="Support_for_Economic_Goals" class="form-label">Support for Economic Goals</label>
                               

                                <br>
                                <label>
                                    <input type="radio" class="" name="input.Support_for_Economic_Goals" id="Support_for_Economic_Goals" value="1" @(Model.WorkCandidates.Support_for_Economic_Goals == 1 ? "checked" : "")> Yes
                                </label>
                                <label>
                                    <input type="radio" class="" name="input.Support_for_Economic_Goals" id="Support_for_Economic_Goals" value="0" @(Model.WorkCandidates.Support_for_Economic_Goals == 0 ? "checked" : "") @(Model.WorkCandidates.Support_for_Economic_Goals == null ? "checked" : "")> No
