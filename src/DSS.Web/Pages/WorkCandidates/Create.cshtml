@page
@model DSS.Web.Pages.WorkCandidates.CreateModel
@{
}
@{
    Layout = "Shared/Layout";
    ViewData["Menu"] = "DSSMenus.Pools";
}


<div class="container-fluid px-4">
    <h1 class="mt-4">
        Work Candidate
    </h1>
   
    <div class="card mb-4">
        <div class="row">
            <div class="col-lg-2 col-md-3 mb-5 offset-10">
                
            </div>
        </div>
        <div class="card-body">

           
               

            <link rel="stylesheet" href="https://code.jquery.com/ui/1.13.3/themes/base/jquery-ui.css">
            <script src="https://code.jquery.com/ui/1.13.3/jquery-ui.js"></script>
            <script>
                $(function () {
                    $("#tabs").tabs({
                        create: function (event, ui) {
                            // Initialize the active tab
                            var activeId = ui.panel.attr("id");
                            $("#tabs ul li:has(a[href='#" + activeId + "'])").addClass(activeId + "-active");
                        },
                        activate: function (event, ui) {
                            var activeId = ui.newPanel.attr("id");
                            // Remove active classes from all tabs
                            $("#tabs ul li").removeClass(function (index, className) {
                                return (className.match(/(^|\s)(tabs-general|tabs-safety|tabs-mobility|tabs-equity|tabs-resilience|tabs-condition|tabs-other)-active/g) || []).join(' ');
                            });
                            // Add the active class to the newly active tab
                            $("#tabs ul li:has(a[href='#" + activeId + "'])").addClass(activeId + "-active");
                        }
                    });
                });
            </script>
           
            @{
                var handlerName = !Model.WorkCandidateId.HasValue || Model.WorkCandidateId.ToString() == ""
                ? "CreateWorkCandidate"
                : "UpdateWorkCandidate";
            }


            <form method="POST" asp-page-handler="@handlerName">
                <input type="hidden" id="WorkCandidateId" name="id" value="@Model.WorkCandidateId" />
                <input type="hidden" id="PoolId" name="PoolId" value="@Model.PoolId" />
            
            <div id="tabs">
                <ul>
                    <li class="general-li"><a href="#tabs-general">General</a></li>
                        <li class="safety-li"><a href="#tabs-safety">Safety</a></li>
                        <li class="mobility-li"><a href="#tabs-mobility">Mobility and Economy</a></li>
                        <li class="equity-li"><a href="#tabs-equity">Equity and Accessbility</a></li>
                        <li class="resilience-li"><a href="#tabs-resilience">Resilience and Environment</a></li>
                        <li class="condition-li"><a href="#tabs-condition">Condition and Performance</a></li>
                        
                            @if (@Model.WorkCandidateId != null)
                            {
                        <li class="other-li"><a href="#tabs-other">Other</a></li>
                            }
                </ul>
                <div id="tabs-general">
                    <div class="row mb-3">
                        <div class="col-lg-2 col-md-3 mb-5">
                        <label for="input1" class="form-label">District</label>
                                <select class="form-control" name="input.District" id="District" required>
                                   <option value="">Select</option>
                                    @foreach (var district in Model.DistrictList)
                                    {
                                        if (Model.WorkCandidates.District == district.DistrictId)
                                        {
                                            <option value="@district.DistrictId" selected>@district.DistrictName</option>
                                        }
                                        else
                                        {
                                            <option value="@district.DistrictId">@district.DistrictName</option>
                                        }
                                    }
                                  
                                </select>
                    </div>
                    <div class="col-lg-2 col-md-3 mb-5">
                        <label for="input1" class="form-label">County</label>
                                <select class="form-control" name="input.Cnty" id="Cnty" required>
                                    <option value="">Select</option>
                                    </select>
                    </div>


                            <div class="col-lg-2 col-md-3 mb-5">
                                <label for="input1" class="form-label">Route</label>
                                <select type="text" class="form-control" name="input.Route" id="Route" value="@Model.WorkCandidates.Route" required>
                                    <option value="">Select</option>
                                </select>
                            </div>

                            <div class="col-lg-2 col-md-3 mb-5">
                                <label for="input1" class="form-label">Section</label>
                                <select type="text" class="form-control" name="input.ToSection" id="Section" required>
                                    <option value="">Select</option>
                                </select>
                            </div>

                             <div class="col-lg-2 col-md-3 mb-5">
                        <label for="input1" class="form-label">Asset Type</label>
                                <select class="form-control" name="input.AssetType" id="AssetType" asp-for="@Model.WorkCandidates.AssetType" required value="@Model.WorkCandidates.AssetType">
                            <option value="">Select</option>
                            <option value="B">Bridge</option>
                            <option value="P">Pavement</option>
                        </select>
                    </div>

                         <div class="col-lg-2 col-md-3 mb-5">
                                <label for="input1" class="form-label">Project Name</label>
                                <select class="form-control" name="input.ProjectName" id="ProjectName" value="@Model.WorkCandidates.ProjectName">
                                    @foreach (var project in Model.ProjectNames)
                                    {
                                        if (Model.WorkCandidates.ProjectName == @project.Replace("Project ", ""))
                                        {
                                            <option selected value="@project.Replace("Project ", "")">@project</option>
                                        }
                                        else
                                        {
                                            <option value="@project.Replace("Project ", "")">@project</option>
                                        }                                        
                                    }
                                    </select>
                            </div>
                   

                 
                   
                  
                           
                            <div class="col-lg-2 col-md-3 mb-5 BRKEY">
                        <label for="input1" class="form-label">BRKEY</label>
                                <select class="form-control" name="input.BRKEY" id="BRKEY" value="@Model.WorkCandidates.BRKEY">
                                    <option value="">Select</option>
                                    </select>
                    </div>
                            <div class="col-lg-2 col-md-3 mb-5 BRIDGE_ID">
                        <label for="input1" class="form-label">BRIDGE ID</label>
                                <input type="text" class="form-control" name="input.BRIDGE_ID" id="BRIDGE_ID" value="@Model.WorkCandidates.BRIDGE_ID" readonly>
                    </div>
                    <div class="col-lg-2 col-md-3 mb-5">
                        <label for="input1" class="form-label">Work Type</label>
                                <select class="form-control" name="Treatment" id="Treatment">
                                    @foreach (var workType in Model.WorkTypeList)
                                    {
                                        if (Model.WorkCandidates.Treatment == workType.WorkType)
                                        {
                                            <option selected value="@workType.WorkType">@workType.WorkType</option>
                                        }
                                        else
                                        {
                                            <option value="@workType.WorkType">@workType.WorkType</option>
                                        }
                                    }
                            
                        </select>
                    </div>
                    <div class="col-lg-2 col-md-3 mb-5">
                        <label for="input1" class="form-label">Description</label>
                                <input type="text" class="form-control" name="input.Description" id="Description" value="@Model.WorkCandidates.Description">
                    </div>
                    <div class="col-lg-2 col-md-3 mb-5">
                        <label for="input1" class="form-label">Cost</label>
                        <div class="input-group"> 
                            <span class="input-group-text">$</span>
                                    <input type="text" class="form-control" name="input.Cost" id="Cost" value="@Model.WorkCandidates.Cost" oninput="validateDecimalInput(this)">
                        </div>
                    </div>
                   

                           
                            <div class="col-lg-2 col-md-3 mb-5">
                                <label for="Year" class="form-label">Year</label>
                                <select class="form-control" name="input.Year" id="Year" value="@Model.WorkCandidates.Year" oninput="validateWholeNumberInput(this)">
                                    <option value="">Select</option>
                                    @for (int year = DateTime.Now.Year; year <= DateTime.Now.Year + 30; year++)
                                    {
                                        <option value="@year">@year</option>
                                    }
                                </select>
                            </div>
                            <div class="col-lg-2 col-md-3 mb-5">
                                <label for="MinYear" class="form-label">Min.Year</label>
                                <select class="form-control" name="input.MinYear" id="MinYear" value="@Model.WorkCandidates.MinYear" oninput="validateWholeNumberInput(this)">
                                    <option value="">Select</option>
                                    @for (int year = DateTime.Now.Year; year <= DateTime.Now.Year + 30; year++)
                                    {
                                        <option value="@year">@year</option>
                                    }
                                </select>
                            </div>
                            <div class="col-lg-2 col-md-3 mb-5">
                                <label for="MaxYear" class="form-label">Max.Year</label>
                                <select class="form-control" name="input.MaxYear" id="MaxYear" value="@Model.WorkCandidates.MaxYear" oninput="validateWholeNumberInput(this)">
                                    <option value="">Select</option>
                                    @for (int year = DateTime.Now.Year; year <= DateTime.Now.Year + 30; year++)
                                    {
                                        <option value="@year">@year</option>
                                    }
                                </select>
                            </div>

                            <div class="col-lg-2 col-md-3 mb-5">
                                <label for="BPN" class="form-label">BPN</label>
                                <select class="form-control" name="input.BPN" id="BPN" value="@Model.WorkCandidates.BPN">
                                </select>
                            </div>

                           

                            @if (@Model.WorkCandidateId != null)
                            {

                                <div class="col-lg-2 col-md-3 mb-5">
                                    <label for="input1" class="form-label">Predicted Scaled Benefit</label>

                                    <div class="input-group">
                                        <span class="input-group-text">$</span>
                                        <input type="text" class="form-control" name="input.Predicted_Scaled_Benefit" id="Predicted_Scaled_Benefit" value="@Model.WorkCandidates.Predicted_Scaled_Benefit" disabled>
                                    </div>

                                </div>

                                <div class="col-lg-2 col-md-3 mb-5">
                                    <label for="input1" class="form-label">Total Score</label>
                                    <input type="text" class="form-control" name="input.Total_Score" id="Total_Score" value="@(Model.WorkCandidates.Total_Score?.ToString("0.00") ?? "")" disabled>
                                </div>
                            }


                        </div>

                </div>
                <div id="tabs-safety">
