                                <label>
                                    <input type="radio" class="" name="input.Consistency_with_Existing_Plans" id="Consistency_with_Existing_Plans" value="1" @(Model.WorkCandidates.Consistency_with_Existing_Plans == 1 ? "checked" : "")> Yes
                                </label>
                                <label>
                                    <input type="radio" class="" name="input.Consistency_with_Existing_Plans" id="Consistency_with_Existing_Plans" value="0" @(Model.WorkCandidates.Consistency_with_Existing_Plans == 0 ? "checked" : "") @(Model.WorkCandidates.Consistency_with_Existing_Plans == null ? "checked" : "")> No
